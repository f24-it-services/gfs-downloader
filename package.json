{
  "name": "gfs-downloader",
  "version": "1.0.2",
  "description": "Helpers for downloading meteorological data from NOOA servers",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register test/*.test.js",
    "dist": "babel src -d lib --ignore node_modules,lib",
    "watch": "npm run dist -- -w",
    "lint": "eslint '**/*.js'"
  },
  "author": "Johannes Klose <jklose@calitrix.com>",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard",
      "promise"
    ]
  },
  "dependencies": {
    "debug": "^2.2.0",
    "gfs-weather-utils": "^1.0.0",
    "lodash.max": "^4.0.1",
    "lodash.padstart": "^4.6.1",
    "node-cache": "^3.2.1",
    "superagent": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.16.3",
    "eslint": "^3.6.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.1.2"
  }
}
