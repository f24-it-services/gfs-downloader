{
  "name": "gfs-downloader",
  "version": "1.0.5",
  "description": "Helpers for downloading meteorological data from NOOA servers",
  "main": "src/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "prepublishOnly": "npm-run-all lint dist test",
    "test": "mocha",
    "dist": "babel src -d lib --ignore node_modules,lib",
    "watch": "npm run dist -- -w",
    "lint": "eslint '**/*.js'"
  },
  "author": "Johannes Klose <jklose@calitrix.com>",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard",
      "promise"
    ]
  },
  "dependencies": {
    "debug": "^4.1.1",
    "gfs-weather-utils": "^1.0.2",
    "lodash.max": "^4.0.1",
    "lodash.padstart": "^4.6.1",
    "node-cache": "^4.2.0",
    "superagent": "^5.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5"
  }
}
