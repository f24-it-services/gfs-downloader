{
  "name": "gfs-downloader",
  "version": "1.1.5",
  "description": "Helpers for downloading meteorological data from NOOA servers",
  "keywords": [],
  "homepage": "https://github.com/f24-it-services/gfs-downloader#readme",
  "bugs": {
    "url": "https://github.com/f24-it-services/gfs-downloader/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f24-it-services/gfs-downloader.git"
  },
  "license": "MIT",
  "author": "Johannes Klose <jklose@calitrix.com>",
  "main": "lib/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "all": "npm-run-all clean lint dist test",
    "clean": "rimraf lib",
    "dist": "babel src -d lib --ignore node_modules,lib",
    "lint": "eslint '**/*.js'",
    "prepublishOnly": "npm run all",
    "test": "mocha -r esm",
    "watch": "npm run dist -- -w"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard",
      "promise"
    ],
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "dependencies": {
    "debug": "^4.3.2",
    "esm": "^3.2.25",
    "gfs-weather-utils": "^1.1.1",
    "lodash.max": "^4.0.1",
    "lodash.padstart": "^4.6.1",
    "node-cache": "^5.1.2",
    "should-proxy": "^1.0.4",
    "superagent": "^6.1.0",
    "superagent-proxy": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.14",
    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "mkdirp": "^1.0.4",
    "mocha": "^8.3.2",
    "mockttp": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
