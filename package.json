{
  "name": "gfs-downloader",
  "version": "1.0.5",
  "description": "Helpers for downloading meteorological data from NOOA servers",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "prepublishOnly": "npm-run-all lint dist test",
    "test": "mocha",
    "dist": "babel src -d lib --ignore node_modules,lib",
    "watch": "npm run dist -- -w",
    "lint": "eslint '**/*.js'"
  },
  "author": "Johannes Klose <jklose@calitrix.com>",
  "license": "MIT",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard",
      "promise"
    ]
  },
  "dependencies": {
    "debug": "^3.0.0",
    "gfs-weather-utils": "^1.0.1",
    "lodash.max": "^4.0.1",
    "lodash.padstart": "^4.6.1",
    "node-cache": "^4.2.0",
    "superagent": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "eslint": "^5.6.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3"
  }
}
